{
  "name": "aimolt",
  "version": "1.0.0",
  "description": "Discord bot with Gemini AI and Supabase",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --inspect=0.0.0.0:9229 src/index.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop aimolt",
    "pm2:restart": "pm2 restart aimolt",
    "pm2:logs": "pm2 logs aimolt",
    "pm2:delete": "pm2 delete aimolt",
    "docker:build": "docker compose build",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
