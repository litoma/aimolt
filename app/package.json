    "axios": "^1.10.0",
    "discord.js": "^14.20.0",
    "dotenv": "^16.5.0",
    "node-cron": "^3.0.3",
    "pg": "^8.13.0"
  },
  "devDependencies": {
    "pm2": "^6.0.8"
  },
